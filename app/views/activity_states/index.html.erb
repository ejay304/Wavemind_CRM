<h1>Listing activity_states</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @activity_states.each do |activity_state| %>
      <tr>
        <td><%= activity_state.name %></td>
        <td><%= link_to 'Show', activity_state %></td>
        <td><%= link_to 'Edit', edit_activity_state_path(activity_state) %></td>
        <td><%= link_to 'Destroy', activity_state, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Activity state', new_activity_state_path %>
